<% if session[:teacher_id] == nil %>
	<br>
	<%= form_for(:session, url: teacher_log_in_path) do |f| %>
		<%= f.label :email %>
		<%= f.search_field :email %>
		<%= f.label :password %>
		<%= f.password_field :password %>
		<%= f.submit "Log in" %>
	<% end %>
	<br>
<% else %>

	<h1>
	<%= "Welcome #{Teacher.find(session[:teacher_id]).name}. " %>
	</h1>


	<%= button_to "Logout", teacher_log_out_path, method: :delete %>
 	
<% end %>